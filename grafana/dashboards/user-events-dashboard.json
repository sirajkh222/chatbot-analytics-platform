      {
        "id": 2,
        "title": "User Journey Funnel",
        "type": "piechart",
        "targets": [
          {
            "rawSql": "SELECT 'Page Loads' as metric, COUNT(\"pageLoaded\") as value FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Widget Opens' as metric, COUNT(\"widgetOpened\") as value FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Human Requests' as metric, COUNT(\"requestedHuman\") as value FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Accept Presses' as metric, COUNT(\"pressedAccept\") as value FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Callback Presses' as metric, COUNT(\"pressedCallback\") as value FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\")",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Conversion Rates",
        "type": "bargauge",
        "targets": [
          {
            "rawSql": "SELECT 'Page → Widget' as conversion, ROUND((COUNT(\"widgetOpened\")::float / NULLIF(COUNT(\"pageLoaded\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Widget → Human Request' as conversion, ROUND((COUNT(\"requestedHuman\")::float / NULLIF(COUNT(\"widgetOpened\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\") UNION ALL SELECT 'Request → Accept' as conversion, ROUND((COUNT(\"pressedAccept\")::float / NULLIF(COUNT(\"requestedHuman\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE $__timeFilter(\"createdAt\")",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 25
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
