{
  "dashboard": {
    "id": null,
    "title": "Maple Community Services - Chatbot Business Analytics",
    "tags": ["maple", "chatbot", "business", "ndis", "leads", "agents"],
    "style": "dark",
    "timezone": "Australia/Sydney",
    "refresh": "5m",
    "time": {
      "from": "now-30d",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "User Journey Funnel - Real Business Metrics",
        "type": "stat",
        "targets": [
          {
            "rawSql": "SELECT 'Page Loads' as metric, COUNT(\"pageLoaded\") as value FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Widget Opens' as metric, COUNT(\"widgetOpened\") as value FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Human Requests' as metric, COUNT(\"requestedHuman\") as value FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Accept Presses' as metric, COUNT(\"pressedAccept\") as value FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Callback Requests' as metric, COUNT(\"pressedCallback\") as value FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\")",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "#FDC200",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 50
                }
              ]
            },
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Conversion Rates - Key Business KPIs",
        "type": "bargauge",
        "targets": [
          {
            "rawSql": "SELECT 'Page → Widget' as conversion, ROUND((COUNT(\"widgetOpened\")::float / NULLIF(COUNT(\"pageLoaded\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Widget → Human Request' as conversion, ROUND((COUNT(\"requestedHuman\")::float / NULLIF(COUNT(\"widgetOpened\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\") UNION ALL SELECT 'Request → Accept/Callback' as conversion, ROUND(((COUNT(\"pressedAccept\") + COUNT(\"pressedCallback\"))::float / NULLIF(COUNT(\"requestedHuman\"), 0)) * 100, 2) as rate FROM \"UserEvents\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"createdAt\")",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 25
                },
                {
                  "color": "#FDC200",
                  "value": 50
                },
                {
                  "color": "green",
                  "value": 75
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Lead Generation Performance - Your Business Results",
        "type": "timeseries",
        "targets": [
          {
            "rawSql": "SELECT $__timeGroup(\"capturedAt\" AT TIME ZONE 'Australia/Sydney','1d') as time, COUNT(*) as \"Total Leads\", COUNT(CASE WHEN source = 'chatbot' THEN 1 END) as \"Chatbot Leads\", COUNT(CASE WHEN source = 'callback' THEN 1 END) as \"Callback Leads\", COUNT(CASE WHEN \"salesforceSynced\" = true THEN 1 END) as \"Synced to Salesforce\" FROM \"Leads\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"capturedAt\") GROUP BY 1 ORDER BY 1",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              }
            },
            "unit": "short",
            "min": 0,
            "color": {
              "mode": "palette-classic"
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Total Leads"
              },
              "properties": [
                {
                  "id": "color",
                  "value": {
                    "mode": "fixed",
                    "fixedColor": "#FDC200"
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "calcs": ["sum", "mean"],
            "displayMode": "table",
            "placement": "bottom"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Agent Performance - Human Handoff Analytics",
        "type": "table",
        "targets": [
          {
            "rawSql": "SELECT \"agentName\", COUNT(*) as \"Total Sessions\", ROUND(AVG(\"waitingDuration\"), 2) as \"Avg Wait Time (s)\", ROUND(AVG(\"sessionDuration\"), 2) as \"Avg Session Length (s)\", COUNT(CASE WHEN \"disconnectionReason\" = 'agent_ended' THEN 1 END) as \"Agent Ended\", COUNT(CASE WHEN \"disconnectionReason\" = 'user_left' THEN 1 END) as \"User Left\", COUNT(CASE WHEN status = 'timeout' THEN 1 END) as \"Timeouts\" FROM \"AgentConnections\" WHERE \"clientId\" = 'maplecommunity' AND $__timeFilter(\"connectedAt\") GROUP BY \"agentName\" ORDER BY COUNT(*) DESC",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "cellOptions": {
                "type": "auto"
              }
            },
            "mappings": [],
