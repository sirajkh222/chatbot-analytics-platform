            {
              "matcher": {
                "id": "byName",
                "options": "Avg Session Length (s)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "cellHeight": "sm",
          "footer": {
